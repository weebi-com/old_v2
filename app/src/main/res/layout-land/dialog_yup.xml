<?xml version="1.0" encoding="utf-8"?><!--
    android:background="@color/pad_numeric_background_color"
-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:whatever="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/pad_numeric_background_color"
    android:orientation="vertical"
    android:padding="16dp">
    <!--    android:inputType="textPersonName"
    <com.andreabaccega.widget.FormEditText
        android:id="@+id/verifcodee"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawablePadding="8dp"
        android:drawableTint="@color/pad_advanced_background_color"
        android:hint="numero client"
        android:inputType="number"
        android:textColor="@android:color/white"
        android:textColorHint="@android:color/darker_gray"
        android:textSize="16sp"
        whatever:emptyErrorString="@string/empty_field"
        whatever:testType="nocheck" />-->
    <TextView
        android:id="@+id/boutid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawablePadding="8dp"
        android:textSize="16sp"
        android:text="ID boutique : "
        android:textColor="#fff"/>
    <TextView
        android:id="@+id/numbout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawablePadding="8dp"
        android:textSize="16sp"
        android:text="Numero boutique : "
        android:textColor="#fff"/>
    <TextView
        android:id="@+id/nombout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawablePadding="8dp"
        android:textSize="16sp"
        android:text="nom boutique : "
        android:textColor="#fff"/>
    <TextView
        android:id="@+id/mail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawablePadding="8dp"
        android:textSize="16sp"
        android:text="mail boutique : "
        android:textColor="#fff"/>
    <TextView
        android:id="@+id/nomclie"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawablePadding="8dp"
        android:textSize="16sp"
        android:text="nom client : "
        android:textColor="#fff"/>
    <TextView
        android:id="@+id/numclie"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawablePadding="8dp"
        android:textSize="16sp"
        android:text="numero client : "
        android:textColor="#fff"/>
    <TextView
        android:id="@+id/date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawablePadding="8dp"
        android:textSize="16sp"
        android:text="date : "
        android:textColor="#fff"/>

    <TextView
        android:id="@+id/total"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawablePadding="8dp"
        android:textSize="16sp"
        android:text="Total : "
        android:textColor="#fff"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/clavn"
        android:visibility="gone">
        <include
            layout="@layout/clavier_num"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/clavl"
        android:visibility="gone">
        <include
            layout="@layout/clavier_lettre"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />
    </LinearLayout>


    <ImageView
        android:id="@+id/oknewref"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_gravity="right|bottom"
        android:layout_marginRight="20dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/ok"
        android:tint="@color/tintgreen" />

</LinearLayout>